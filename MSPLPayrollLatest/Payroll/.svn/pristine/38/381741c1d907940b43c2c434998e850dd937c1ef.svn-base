@{
    ViewBag.Title = "Emp_LanguageKnownList";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_HomeLayout.cshtml";
}
<h4>
    Language Known
</h4>
<div class="row">
    <div class="col-md-12">
        <div class="form-group">
            <input type="button" id="btnAddLanguageKnown" value="Add" class="btn custom-button btnRight"
                   data-toggle="modal" data-target="#AddLanguageKnown">
        </div>
    </div>
    <div class="col-md-12">
        <div class="form-group table-responsive">
            <table id="tblLanguageKnown" class="table table-responsive table-striped table-hover table-condensed userTablehand dataTable no-footer">
                <thead>
                    <tr>
                        <th class="nodisp">
                        </th>
                        <th class="nodisp">
                        </th>
                        <th>
                            Language
                        </th>
                        <th>
                            Speak
                        </th>
                        <th>
                            Read
                        </th>
                        <th>
                            Write
                        </th>

                        <th>
                            Action
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>

                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div id="AddLanguageKnown" class="modal fade" role="dialog">
        @Html.Partial("~/Views/Employee/Emp_LanguageKnown.cshtml")
    </div>
</div>
<script src="~/Js/Emp/languageKnown.js" type="text/javascript"></script>
@section Scripts
{
    <script src="~/Js/Emp/languageKnown.js" type="text/javascript"></script>
}
