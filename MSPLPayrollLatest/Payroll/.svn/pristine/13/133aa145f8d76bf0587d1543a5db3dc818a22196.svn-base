<!-- Modal -->
<form id="frmLICPremiumEntry" data-toggle="validator" role="form">
    <div id="LICPremiummodel" class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    &times;
                </button>
                <h4 class="modal-title" id="H4">
                    LIC Premium 
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-horizontal">
                    <div class="col-md-12">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="control-label col-md-6">
                                    Policy Number <label style="color:red;font-size: 13px">*</label>
                                </label>
                                <div class="col-md-6">
                                    <input type="text" id="LICpremiumPolicyno" oncopy="return false" onpaste="return false" readonly="readonly" maxlength="20" class="form-control" placeholder="policy number" autofocus required />
                                </div>
                            </div>
                        </div>
                   </div>
                        <div class="col-md-12">
                            <div class="col-md-6">       
                                <div class="form-group">
                                    <label class="control-label col-md-6">
                                        Premium amount <label style="color:red;font-size: 13px">*</label>
                                    </label>
                                    <div class="col-md-6">
                                        <input type="text" id="LICpremiumamt" oncopy="return false" onpaste="return false" onkeypress="return $validator.checkDecimal(event, 2)" maxlength="20" class="form-control" placeholder="Enter the amount" />
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">                               
                                <div class="form-group">
                                    <label class="control-label col-md-6">
                                        Paid Date <label style="color:red;font-size: 13px">*</label>
                                    </label>
                                    <div class="col-md-6">
                                        <input type="text" id="LICpremiumpolicydate" class="form-control datepicker required" placeholder="Enter the paid Date" readonly />
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-12 form-group">
                                <table id="LICPolicyPremiumformtbl" class="table table-responsive table-striped table-hover table-condensed userTablehand">
                                    <thead>
                                        <tr>
                                            <th class="nodisp">
                                            </th>
                                            <th>
                                                Premium amount
                                            </th>
                                            <th>
                                                Date of Premium paid
                                            </th>
                                            <th>
                                                Action
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td></td>
                                            <td></td>
                                            <td></td>
                                            <td></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>


            <div class="modal-footer">
                <button type="submit" id="btnLICPremiumpolicysave" onclick="savelicpremiumpolicy()" class="btn custom-button">
                    Save
                </button>
                <button type="button" class="btn custom-button" onclick="closemodel()">
                    Close
                </button>
            </div>


        </div>
    </div>
        </div>
</form>
<style>
   #LICPremiummodel {
        width: 60%;
    }
</style>
<script type="text/javascript" src="~/Js/Tax/declaractionEntry.js"></script>

<script language="javascript">
    debugger;

    $("#PremiumfallingYear").text(new Date($declaractionEntry.financeYear.EndDate).getFullYear().toString());
    $("#PremiumYear").text($declaractionEntry.financeYearDuration());
    $('.datepicker').datepicker({
        calendarWeeks: true,
        changeYear: true,
        changeMonth: true,
        autoclose: true,
        todayHighlight: true,
        dateFormat: 'dd/M/yy',
        yearRange: "-120:+2",
        beforeShowMonth: function (date) {
            //                switch (date.getMonth()) {
            //                    case 8:
            //                        return false;
            //                }
        },
        onSelect: function (dateText, inst) {
            debugger;
            if ($LICpremium.Lifeinsurancepolicylist != null) {
                for (var m = 0; m < $LICpremium.Lifeinsurancepolicylist.length; m++) {
                    if (dateText == $LICpremium.Lifeinsurancepolicylist[m].PremiumDate) {
                        $app.showAlert("Paid date already exist", 3);
                        $('#LICpremiumpolicydate').val('');
                    }
                }
            }
        },
        toggleActive: true
    });

    function closemodel() {
        debugger;
        $('#AddLICFormPremium').modal('hide');
        $LICpremium.Lifeinsurancepolicylist = null;
    }
    function savelicpremiumpolicy()
    {
        $LICpremium.SaveLICPremiumpolicy();
    }
</script>
