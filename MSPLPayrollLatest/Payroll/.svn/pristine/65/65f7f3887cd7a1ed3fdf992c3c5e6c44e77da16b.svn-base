        var temp=$("#txtOfficeEmail").val()  
        if(temp==null){
           
        }
        else {
            $validator.checkEmail('txtOfficeEmail')
        }
    });
    $("#txtPersonalEmail").change(function () {
        var temp = $("#txtPersonalEmail").val()
        if (temp == null) {

        }
        else {
            $validator.checkEmail('txtPersonalEmail')
        }
    });
