
<h2>Pending Leave</h2>


<div class="row">
    <div class="col-md-12">
        <div class="form-group col-md-12 text-right">

            <input type="button" id="btnReportExport" value="Export" class="btn custom-button nodisp">

        </div>
    </div>
    <div class="col-md-12">
        <table id="tblPendingLeaveReport" class="table table-responsive table-striped table-hover table-condensed userTablehand dataTable no-footer">
            <thead>
                <tr>
                    <th class="nodisp"></th>
                    <th>From Date</th>
                    <th>From Day</th>
                    <th>To Date</th>
                    <th>To Day</th>
                    <th>No Of Days</th>
                    <th>LeaveType</th>
                    <th>Applied Reason</th>
                    <th>View Status</th>


                </tr>
            </thead>
            <tbody>
                <tr>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    
                    
                </tr>
            </tbody>
        </table>
    </div>
    <div class="form-group">
        <input id="hdnEmpId"
               name="hiddenId"
               type="hidden"
               value="@Session["EmployeeId"]" />
        <input id="status"
               name="hiddenId"
               type="hidden"
               value="0" />
    </div>
</div>
