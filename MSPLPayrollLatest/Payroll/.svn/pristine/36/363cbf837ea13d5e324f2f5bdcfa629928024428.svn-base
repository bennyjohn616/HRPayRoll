@{
    ViewBag.Title = "EmployeeList";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_HomeLayout.cshtml";
}
<h2>
    Employee
</h2>

<div class="col-md-12 text-right">
    <input type="button" id="btnAddempHide" class="nodisp marginbt7" data-target="#AddEmployee" aria-controls="new"
           role="tab" data-toggle="tab" />
    <input type="button" id="btnAddEmployee" value="Add" class="btn custom-button marginbt7" >
    <input type="button" id="btnBack" value="Back to List" class="btn custom-button hide marginbt7"
           data-target="#employees" aria-controls="employees" role="tab" data-toggle="tab" />
</div>
<div class="tab-content">

    <div role="tabpanel" class="tab-pane active clearfix" id="employees">
        <div class="row">

            <div class="col-md-12 nodisp text-right">
                <input type="text" id="txtSearch" class="" placeholder="Search..." >
            </div>
            <div class="col-md-12">
                <div class="col-md-4">
                    <select id="sltLookup" class="form-control">
                        <option value="0">-Search-</option>
                        <option value="branch">Branch</option>
                        <option value="designation">Designation</option>
                        <option value="costcenter">Cost Centre</option>
                        <option value="esilocation">ESI Location</option>
                        <option value="grade">Grade</option>
                        <option value="category">Category</option>
                        <option value="dept">Department</option>
                        <option value="location">Location</option>
                        <option value="ptlocation">PT Location</option>
                        <option value="empCode">Employee Code</option>
                        <option value="empFName">First Name</option>
                        <option value="empLName">Last Name</option>
                        <option value="empDOJ">DOJ</option>
                    </select>
                </div>
                <div class="col-md-4">
                    <input type="text" id="txtSearchData" role="textbox" class="input-elm form-control"
                           style="width: 98%;" />
                </div>
                <div class="col-md-3 form-group">
                    <input class="btn custom-button" value="Search" type="button" onclick="$employee.searchData('search')" id="btnSearch" />
                    <input class="btn custom-button" value="Cancel" type="button" onclick="$employee.searchData('clear')" id="btnClear" />
                </div>
            </div>
            <div class="col-md-12 table-responsive">
                <table id="tblemployee" class="table table-responsive table-striped table-hover table-condensed userTablehand dataTable no-footer">

                    <thead>
                        <tr>
                            <th class="nodisp">
                            </th>
                            <th>
                                Employee Code
                            </th>
                            <th>
                                First Name
                            </th>
                            <th>
                                Last Name
                            </th>
                            <th>
                                Email
                            </th>
                            <th>
                                Phone
                            </th>
                            <th>
                                DOB
                            </th>
                            <th>
                                DOJ
                            </th>
                            <th>
                                Department
                            </th>
                            <th>
                                Designation
                            </th>
                            <th>
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div role="tabpanel" class="tab-pane fade clearfix" id="AddEmployee">
        @Html.Partial("~/Views/Employee/Employee.cshtml")
    </div>
</div>
<script src="~/Js/Emp/EmployeeDetails.js"></script>
<script src="~/Js/Company/CompanyCom.js"></script>
<script src="~/Js/Company/entityMapping.js"></script>
<script src="~/Js/Trans/increment.js"></script>
<script src="~/Js/Trans/StopPayment.js"></script>
<script src="~/Js/Loan/LoanEntry.js"></script>
<script src="~/Js/Company/joiningDoc.js"></script>
<script src="~/Js/Setting/Setting.js"></script>
<script src="~/Js/Company/HRComponent.js"></script>
@*<script src="~/Js/application.js" type="text/javascript"></script> --*@
 
@section Scripts
   
{
    <script src="~/Js/Emp/EmployeeDetails.js"></script>
    <script src="~/Js/Company/CompanyCom.js" type="text/javascript"></script>
    <script src="~/Js/Company/entityMapping.js"></script>
    <script src="~/Js/Trans/increment.js"></script>
    <script src="~/Js/Trans/StopPayment.js"></script>
    <script src="~/Js/Loan/LoanEntry.js"></script>
    <script src="~/Js/Company/joiningDoc.js"></script>
    <script src="~/Js/Setting/Setting.js"></script>
    <script src="~/Js/Company/HRComponent.js"></script>
}
