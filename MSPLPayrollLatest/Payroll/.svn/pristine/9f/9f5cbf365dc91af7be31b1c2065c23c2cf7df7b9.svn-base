    <script language="javascript">
        $(window).resize(function () {

            $('.wraplist li>a').each(function () {
                if ($(window).width() > 1024) {
                    $(this).children('span.hidden').removeClass("hidden");
                }
            });
        });

        $('.datepicker').datepicker({
            calendarWeeks: true,
            autoclose: true,
            todayHighlight: true,
            beforeShowMonth: function (date) {
                switch (date.getMonth()) {
                    case 8:
                        return false;
                }
            },
            toggleActive: true
        });

    </script>
