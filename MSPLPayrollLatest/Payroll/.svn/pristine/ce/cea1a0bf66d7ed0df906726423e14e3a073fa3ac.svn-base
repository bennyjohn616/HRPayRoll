<h2>
    Joining Document</h2>
<div class="row" >
    <div class="col-md-12 text-right">
        <input type="button" id="btnSaveJoinDoc" value="Add" class="btn custom-button marginbt7" data-toggle="modal"
            data-target="#AddJoinDocument" >
    </div>
    <div class="col-md-12 table-responsive">
        <table id="tblJoinDoc" class="table table-responsive table-striped table-hover table-condensed userTablehand dataTable no-footer">
            <thead>
                <tr>
                    <th class="nodisp">
                    </th>
                    <th>
                        Document Name
                    </th>
                    <th>
                        Company Id
                    </th>
                   
                    <th>
                        Action
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                    </td>
                    <td>
                    </td>
                    <td>
                    </td>
                    <td>
                    </td>
                   
                </tr>
            </tbody>
        </table>
    </div>
    <div id="AddJoinDocument" class="modal fade" role="dialog" aria-hidden="true" data-backdrop="static"
         data-keyboard="false" >
        @Html.Partial("~/Views/Company/JoinDocument.cshtml")
    </div>
</div>
<script src="~/Js/Company/joiningDoc.js" type="text/javascript"></script>
@section Scripts 
{
    <script src="~/Js/Company/joiningDoc.js" type="text/javascript"></script>
}
