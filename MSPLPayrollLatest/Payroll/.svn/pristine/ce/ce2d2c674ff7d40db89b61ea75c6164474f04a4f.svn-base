

<h2 class="title">LOPCredit</h2>
<form id="frmCredit">
    <div class="col-md-8">
        <div class="form-group pull-right">
            <button type="button" id="btnLopSave" class="btn custom-button">Save</button>
        </div>
    </div>

    <div id="dvCredit" class="row">

        <input id="hdnType" type="hidden" value="LOP" />
        <div class="col-md-8">
            <div class="form-horizontal">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label class="control-label col-md-4">
                            Category
                        </label>
                        <div class="col-md-6">
                            <select id="sltCategorylist" class="form-control" autofocus></select>
                        </div>
                    </div>

                </div>
                <div class=" " id="dvemp">
                    <div class="col-sm-12">
                        <div class="form-group">
                            <label class="control-label col-sm-4">
                                <label>Apply Month</label>
                            </label>
                            <div class="col-sm-6">
                                <select Id="ddMonth" class="form-control" required>
                                    <option value="1">January</option>
                                    <option value="2">February</option>
                                    <option value="3">March</option>
                                    <option value="4">April</option>
                                    <option value="5">May</option>
                                    <option value="6">June</option>
                                    <option value="7">July</option>
                                    <option value="8">August</option>
                                    <option value="9">September</option>
                                    <option value="10">October</option>
                                    <option value="11">November</option>
                                    <option value="12">December</option>

                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-12">
                        <div class="form-group">
                            <label class="control-label col-sm-4">
                                <label>Apply Year</label>
                            </label>
                            <div class="col-sm-6">
                                @*<input type="text" onkeypress="return $validator.IsNumeric(event, this.id)" maxlength="4" min="4" class="form-control" id="txtYear" required />*@
                                <select id="txtYear" class="form-control" autofocus></select>
                            </div>
                        </div>
                    </div>
                    <div id="divEmp" class="col-sm-12">
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Employee Code
                            </label>
                            <div class="col-md-6">
                                <select id="sltEmployeelist" class="form-control"></select>
                            </div>
                        </div>
                    </div>


                    <div class="col-sm-12">
                        <div class="form-group table-responsive">
                            <div id="dvtbl">

                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>

    </div>
</form>



<script>
    $companyCom.loadYear({ id: "txtYear" });
</script>
<script src="~/Js/Company/CompanyCom.js" type="text/javascript">
</script>
<script src="~/Js/Trans/lopCredit.js"></script>
<script src="~/Js/Trans/supplementary.js"></script>


