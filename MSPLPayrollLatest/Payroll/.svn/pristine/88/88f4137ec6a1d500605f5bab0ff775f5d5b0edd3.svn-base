                                            <select id="medSeniorcitizen" class="form-control Reqrd">
                                                <option value="1">Senior Citizen</option>
                                                <option value="2">Normal</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-6">
