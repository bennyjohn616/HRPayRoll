
@{
    ViewBag.Title = "EmployeeLeaveReport";
}
<div class="form-horizontal">
    <ul class="nav nav-tabs" id="empLeaveNavtabs">

        <li><a href="#leaveReport" data-toggle="tab" class="empTab" id="FullLeaveReport">Leave Report</a></li>
        <li><a href="#leavePendingReport" data-toggle="tab" class="empTab" id="PendingReport">Pending Report</a></li>
        <li><a href="#leaveApprovedReport" data-toggle="tab" class="empTab" id="ApprovedReport">Approved Report</a></li>
        <li><a href="#leaveRejectedReport" data-toggle="tab" class="empTab" id="RejectedReport">Rejected Report</a></li>
        <li><a href="#leaveCancelledReport" data-toggle="tab" class="empTab" id="CancelledReport">Cancelled Report</a></li>
    </ul>
    <center><h4> <label id="lblHederCompgainText" class="control-label" style="font-size:larger"></label></h4></center>
    <label id="lblhiddenNavtabvalue" class="control-label nodisp"></label>
    <div class="form-group">
        <input id="hdnEmpId" name="hiddenId" type="hidden" value="@Session["EmployeeId"]" />
        <input type="hidden" id="hdnRoleName" value="@Session["RoleName"]" />
    </div>
</div>

<div class="tab-content" id="tabcontent">

    <div id="leaveReport" class="tab-pane fade">
        <div class="col-md-12 ">
            <div class="form-group col-md-6 ">
                <h2>Leave Report</h2>
            </div>
            <div class="form-group col-md-6 ">
                <div style="float:right">
                    <input type="button" id="btnReportView" value="View" onclick="$EmployeeLeaveReport.LoadEmployeeLeaveReport()" class="btn custom-button">
                    <input type="button" id="btnEMPReportExport" value="Export" onclick="$EmployeeLeaveReport.Excelreportexport(data='MyReport')" class="btn custom-button ">
                </div>
            </div>
        </div>


        <div class="col-md-12">
            <div class="form-group col-md-4 ">
                <label class="control-label col-md-5">
                    From Date <label style="color:red;font-size: 13px">*</label>
                </label>
                <div class="col-md-7">
                    <input type="text" id="txtReportFromDate" class="form-control datepicker" placeholder="From Date"
                           readonly="readonly" />
                </div>
            </div>
            <div class="form-group col-md-4 ">
                <label class="control-label col-md-4">
                    To Date <label style="color:red;font-size: 13px">*</label>
                </label>
                <div class="col-md-7">
                    <input type="text" id="txtReportToDate" class="form-control datepicker " placeholder="To Date" readonly="readonly" />
                </div>
            </div>
            <div class="form-group col-md-4 ">
                <label class="control-label col-md-4">
                    Status <label style="color:red;font-size: 13px">*</label>
                </label>
                <div class="col-md-7">
                    <select class="form-control" id="sltleavestatus">
                        <option value="99">All Leave</option>
                        <option value="0">Pending Leave</option>
                        <option value="1">Approved Leave</option>
                        <option value="2">Rejected Leave</option>
                        <option value="3">Cancelled Leave</option>
                        <option value="5">HR Approved Cancelled </option>

                    </select>
                </div>
            </div>
        </div>
        <div id="reportdivid" class="nodisp">




            <label id="lblLevTitle" for="myvalue" style="vertical-align: middle"></label>
            
            <div class="accordion-content default">

                <div class="col-md-12">
                    <div class="form-group">
                        <table id="tblemployeeleaveViewReport" class="table table-responsive table-striped table-hover table-condensed userTablehand dataTable no-footer">
                            <thead>
                                <tr>
                                    <th class="nodisp">
                                    </th>
                                    <th>
                                        Employee Code
                                    </th>
                                    <th>
                                        Employee Name
                                    </th>
                                    <th>
                                        From Date
                                    </th>
                                    <th>
                                        From Day
                                    </th>
                                    <th>
                                        To Date
                                    </th>
                                    <th>
                                        To Day
                                    </th>
                                    <th>
                                        No Of Days
                                    </th>
                                    <th>
                                        Leave Type
                                    </th>
                                    <th>
                                        Leave Status
                                    </th>
                                    <th>
                                        View Status
                                    </th>


                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>

                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>



            <div id="ViewStatusPopupFull" class="modal fade" role="dialog" aria-hidden="true" data-backdrop="static"
                 data-keyboard="false">
                @Html.Partial("~/Views/Leave/PendingStatusList.cshtml")
            </div>
            
        </div>
        

        @*<script>
                //$(".datepicker").datepicker(
                //{

                //    changeMonth: true,
                //    changeYear:true
                //});
            $(".nodisp").nodisp (
                {
            display: none

                });



            </script>*@
        <script src="~/Js/PayrollMain.js"></script>
        <script src="~/Js/Leave/EmployeeLeaveReport.js"></script>

    </div>
    @*<div id="ViewStatusPopup" class="modal fade" role="dialog" aria-hidden="true" data-backdrop="static"
         data-keyboard="false">
        @Html.Partial("~/Views/Leave/PendingStatusList.cshtml")
    </div>*@
    <div id="ViewStatusPopup" class="modal fade" role="dialog" aria-hidden="true" data-backdrop="static"
         data-keyboard="false">
        <form id="frmPendingStat" data-toggle="validator" role="form">
            <div class="modal-dialog">

                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">
                            &times;
                        </button>
                        <h4 class="modal-title" id="H4">
                            <label id="PopupTitle">Leave Status</label>
                        </h4>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-12">
                                <table id="tbllevstatusDetail" class="table table-responsive table-striped table-hover table-condensed userTablehand dataTable no-footer">
                                    <thead>
                                        <tr>

                                            <th>Manager Code</th>
                                            <th>Manager Name</th>
                                            <th>Priority Number</th>
                                            <th>Leave Status</th>


                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td></td>
                                            <td></td>
                                            <td></td>
                                            <td></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                    <div class="modal-footer">

                        <button type="button" class="btn custom-button" data-dismiss="modal">
                            Close
                        </button>
                    </div>
                </div>
            </div>
        </form>
    </div>
    <div id="leavePendingReport" class="tab-pane fade">
        @Html.Partial("~/Views/Leave/PendingLeave.cshtml")
    </div>
    <div id="leaveApprovedReport" class="tab-pane fade">
        @Html.Partial("~/Views/Leave/ApprovedLeave.cshtml")
    </div>
    <div id="leaveRejectedReport" class="tab-pane fade">
        @Html.Partial("~/Views/Leave/RejectedLeave.cshtml")
    </div>
    <div id="leaveCancelledReport" class="tab-pane fade">
            @Html.Partial("~/Views/Leave/CancelledLeave.cshtml")
        </div>
    
</div>
<div id="divApprovedCancelReason"></div>
<script src="~/Js/Leave/LeaveReport.js" type="text/javascript"></script>
<script src="~/Js/Company/CompanyCom.js" type="text/javascript"></script>
<script src="~/Js/Leave/EmployeeLeaveReport.js" type="text/javascript"></script>
