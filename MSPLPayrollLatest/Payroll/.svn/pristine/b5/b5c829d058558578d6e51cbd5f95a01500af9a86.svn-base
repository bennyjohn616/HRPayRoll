<!-- Modal -->
<form id="frmFinancialYear" data-toggle="validator" role="form" action="javascript:$category.save()">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    &times;
                </button>
                <h4 class="modal-title" id="H4">
                    Add/Edit Slab
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="control-label col-md-4">
                            Slab <label style="color:red;font-size: 13px">*</label>
                        </label>
                        <div class="col-md-7">
                            <select id="ddSlab">
                                <option>--Select--</option>
                                <option>Abc</option>
                                <
                                <option>Def</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-4">
                            Range From <label style="color:red;font-size: 13px">*</label>
                        </label>
                        <div class="col-md-7">
                            <input type="text" id="txtRangeFrom" class="form-control" placeholder="Enter The Range From "
                                   maxlength="3" required />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-4">
                            Range To <label style="color:red;font-size: 13px">*</label>
                        </label>
                        <div class="col-md-7">
                            <input type="text" id="txtRangeTo" class="form-control" placeholder="Enter The Range To "
                                   maxlength="3" required />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-4">
                            Percentage <label style="color:red;font-size: 13px">*</label>
                        </label>
                        <div class="col-md-7">
                            <input type="text" id="txtPercentage" class="form-control" placeholder="Enter The Percentage"
                                   maxlength="20" autofocus required />
                        </div>
                    </div>
                    
                </div>
            </div>
            <div class="modal-footer">
                <button type="submit" id="btnSave" class="btn custom-button">
                    Save
                </button>
                <button type="button" class="btn custom-button" data-dismiss="modal">
                    Close
                </button>
            </div>
        </div>
    </div>
</form>

<script type="text/javascript" src="~/Js/Tax/Slab.js"></script>
<script type="text/javascript" src="~/Js/Company/CompanyCom.js"></script>
