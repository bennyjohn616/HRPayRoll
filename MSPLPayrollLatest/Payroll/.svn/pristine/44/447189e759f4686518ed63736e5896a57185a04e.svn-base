<!-- Modal -->
<form id="frmFinancialYear" data-toggle="validator" role="form" action="javascript:$financeYear.save()">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    &times;
                </button>
                <h4 class="modal-title" id="H4">
                    Add/Edit Finance Year
                </h4>
            </div>
            <div class="modal-body">
                <div class="col-sm-12">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Start Date <label style="color:red;font-size: 13px">*</label>
                            </label>
                            <div class="col-md-7">
                                <input type="text" id="txtStartDate" class="form-control datepicker" readonly="readonly"  placeholder="Select Date"
                                       maxlength="20" required />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                End Date <label style="color:red;font-size: 13px">*</label>
                            </label>
                            <div class="col-md-7">
                                <input type="text" id="txtEndDate" class="form-control datepicker"  readonly="readonly" placeholder="Select Date"
                                       maxlength="10" required  disabled/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Tan No <label style="color:red;font-size: 13px">*</label>
                            </label>
                            <div class="col-md-7">
                                <input type="text" id="txtTanNo" class="form-control" placeholder="Enter the  Tan No "
                                       maxlength="20" required />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                TDS Circle <label style="color:red;font-size: 13px">*</label>
                            </label>
                            <div class="col-md-7">
                                <input type="text" id="txtTdsCircle" class="form-control" placeholder="Enter the TDS Circle"
                                       maxlength="20" autofocus required />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                PAN/GIR No <label style="color:red;font-size: 13px">*</label>
                            </label>
                            <div class="col-md-7">
                                <input type="text" id="txtPANno" class="form-control" placeholder="Enter the PAN/GIR No"
                                       maxlength="20" required />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Tax Deduction A/C No <label style="color:red;font-size: 13px">*</label>
                            </label>
                            <div class="col-md-7">
                                <input type="text" id="txtTaxDedAccNo" class="form-control" placeholder="Enter the Tax Deduction Acc No"
                                       maxlength="20" required />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Employee Incharge<label style="color:red;font-size: 13px">*</label>
                            </label>
                            <div class="col-md-7">
                                <select id="sltEmployee" class="form-control"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                Place <label style="color:red;font-size: 13px">*</label>
                            </label>
                            <div class="col-md-7">
                                <input type="text" id="txtPlace" class="form-control" placeholder="Enter the Place"
                                       maxlength="20" required />
                            </div>
                        </div>
                        <div class="form-group">

                            <label class="control-label col-md-6">
                                <input type="checkbox" id="chkDefault" />   Set as Default FinanceYear
                            </label>

                        </div>
                    </div>
                </div>
                <div class="nodisp col-sm-6">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <table id="tblCess" class="table">
                                <thead>
                                    <tr>
                                        <th class="nodisp">id</th>
                                        <th>Cess</th>
                                        <th>Percentage</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tfoot>
                                    <tr>
                                        <td></td>
                                        <td><input class="form-control" type="text" id="txtCessName" /></td>
                                        <td><input class="form-control" type="text"  onkeypress="return $validator.IsNumeric(event, this.id)" id="txtCessPercentage" /></td>
                                        <td><a href="#" class="editeButton" title="Add" onclick="$financeYear.addCess()"><span aria-hidden="true" class="glyphicon glyphicon-plus"></span></a></td>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>

                        <div class="form-group">
                            <label class="control-label">
                                House Rent Allowance Exemption
                            </label>
                            <table id="tblHRAExemption">
                                <thead>
                                    <tr>
                                        <th class="nodisp">id</th>
                                        <th>Cess</th>
                                        <th>Percentage</th>

                                    </tr>
                                </thead>
                            </table>
                        </div>
                        <div class="form-group">
                            <label class="control-label">Of Basic + </label>
                            <table id="tblTaxable">

                                <thead>

                                    <tr>
                                        <th>id</th>
                                        <th>Name</th>
                                        <th class="nodisp">exampId</th>

                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="submit" id="btnSave" class="btn custom-button">
                    Save
                </button>
                <button type="button" class="btn custom-button" data-dismiss="modal">
                    Close
                </button>
            </div>
        </div>
    </div>
</form>




