@{
    ViewBag.Title = "Emp_ResignationList";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_HomeLayout.cshtml";
}
<div class="row box">
    <div class="title">
        <h2>Resignation</h2>
    </div>
    <div class="col-md-12 form-group">
        <div class="col-md-12 form-group">
            <input type="button" id="btnAddResignation" value="Add" class="btn custom-button btnRight"
                   data-toggle="modal" data-target="#AddResignation">
        </div>
        <div class="col-md-12 table-responsive">
            <table id="tblResignationEmployee" class="table table-responsive table-striped table-hover table-condensed userTablehand dataTable no-footer">
                <thead>
                    <tr>
                        <th class="nodisp">
                        </th>
                        <th class="nodisp">
                        </th>
                        <th class="nodisp">
                        </th>
                        <th>
                            Category
                        </th>
                        <th>
                            Employee Code
                        </th>
                        <th>
                            Last Working Date
                        </th>
                        <th>
                            Resignation Date
                        </th>
                        <th>
                            Resignation Reason
                        </th>
                        <th>
                            Action
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div id="AddResignation" class="modal fade" role="dialog" aria-hidden="true" data-backdrop="static"
         data-keyboard="false">
        @Html.Partial("~/Views/Transaction/Resignation.cshtml")
    </div>
</div>

<script src="~/Js/Trans/Resignation.js" type="text/javascript"></script>
<script src="~/Js/Company/CompanyCom.js" type="text/javascript"></script>

@section Scripts
{
    <script src="~/Js/Trans/Resignation.js" type="text/javascript"></script>
    <script src="~/Js/Company/CompanyCom.js" type="text/javascript"></script>
}