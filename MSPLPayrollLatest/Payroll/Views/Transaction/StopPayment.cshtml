@{
    ViewBag.Title = "CompanyView";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_HomeLayout.cshtml";
}
<div class="row">
    <div class="col-md-6">
        <div class="form-horizontal">
            <div class="form-group">
                <label class="control-label col-md-4">
                    Category
                </label>
                <div class="col-md-6">
                    <select id="sltCategorylist" class="form-control" autofocus></select>
                </div>
            </div>
            <div class="form-group" id="dvEmpCode">
                <label class="control-label col-md-4">
                    Employee Code
                </label>
                <div class="col-md-6">
                    <select id="sltEmployeelist" class="form-control"></select>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="row" id="transHtml">

</div>
<script src="~/js/Company/CompanyCom.js"></script>
<script src="~/js/trans/StopPayment.js"></script>

