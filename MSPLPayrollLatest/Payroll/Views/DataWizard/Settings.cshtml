
<form id="frmFilter">

    <div class="row">
        <div class="form-horizontal">
            <div class="col-sm-6">
                <div class="col-md-12">
                    <div class="form-group">
                        <input type="radio" name="Matching" id="rdCumulative" value="Cumulative" ><text id="txtCumulative">Cumulative</text>
                        <input type="radio" name="Matching" id="rdMatchings" value="Matching"><text id="txtMatching">Matchings</text>
                        <input type="radio" name="Matching" id="rdNill" value="Nill" checked><text id="txtNill">Nill</text>
                    </div>
                  
                </div>
            </div>
            <div id="dvCumulative" class="hide">

                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-6 control-label">
                            <label>Cumulative Month</label>
                        </label>
                        <div class="col-sm-6">
                            <select id="sltCumlativeMonth" class="form-control">
                                <option value="0">None</option>
                                <option value="1">January</option>
                                <option value="2">February</option>
                                <option value="3">March</option>
                                <option value="4">April</option>
                                <option value="5">May</option>
                                <option value="6">June</option>
                                <option value="7">July</option>
                                <option value="8">August</option>
                                <option value="9">September</option>
                                <option value="10">October</option>
                                <option value="11">November</option>
                                <option value="12">December</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div id="dvMatching" class="hide">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="col-sm-6">
                            <div class="row">
                                <div class="paddingBottom20">
                                    <div class="form-group">
                                        <div class="col-md-12" id="dvColoumn">
                                        </div>

                                    </div>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


</form>
<script src="~/Js/DataWizard/Filter.js"></script>
