                            <label class="control-label col-md-4">
                                April
                            </label>
                            <div class="col-md-4">
                                <input type="text" id="4" class="invent metro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                            <div class="col-md-4">
                                <input type="text" id="4" class="invent nonmetro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                May
                            </label>
                            <div class="col-md-4">
                                <input type="text" id="5" class="invent metro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                            <div class="col-md-4">
                                <input type="text" id="5" class="invent nonmetro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                June
                            </label>
                            <div class="col-md-4">
                                <input type="text" id="6" class="invent metro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                            <div class="col-md-4">
                                <input type="text" id="6" class="invent nonmetro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
