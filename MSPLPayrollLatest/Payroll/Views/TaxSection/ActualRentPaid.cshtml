<form id="frmRentPaid" data-toggle="validator" role="form" action="">
    <div class="modal-dialog"style="width: 60% ;height:85%">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    &times;
                </button>
                <h4 class="modal-title" id="H4">
                    Actual Rent Paid<span id="fYear">blue</span>
                </h4>

            </div>
            <div class="modal-body">
                <div class="col-sm-12">
                    <div class="form-horizontal">
                        <div class="form-group text-right">
                            <div class="col-md-4">
                                <label class="control-label text-right">
                                    Name Of The Landlord :
                                </label>
                            </div>
                            <div class="col-md-8">
                                <input type="text" id="landLordName" class="form-control" maxlength="300" />
                            </div>
                        </div>
                        <div class="form-group text-right">
                            <div class="col-md-4">
                                <label class="control-label text-right">
                                    Address Of The Landlord :
                                </label>
                            </div>
                            <div class="col-md-8">
                                <input type="text" id="landLordAddress" class="form-control" maxlength="300" />
                            </div>
                        </div>
                        <div class="form-group ">
                            <div class="col-md-12 text-center">
                                <label class="control-label ">
