                            <div class="col-md-4">
                                <input type="text" id="7" class="invent metro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                            <div class="col-md-4">
                                <input type="text" id="7" class="invent nonmetro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                August
                            </label>
                            <div class="col-md-4">
                                <input type="text" id="8" class="invent metro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                            <div class="col-md-4">
                                <input type="text" id="8" class="invent nonmetro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                September
                            </label>
                            <div class="col-md-4">
                                <input type="text" id="9" class="invent metro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                            <div class="col-md-4">
                                <input type="text" id="9" class="invent nonmetro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                October
                            </label>
                            <div class="col-md-4">
                                <input type="text" id="10" class="invent metro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                            <div class="col-md-4">
                                <input type="text" id="10" class="invent nonmetro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                November
                            </label>
                            <div class="col-md-4">
                                <input type="text" id="11" class="invent metro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                            <div class="col-md-4">
                                <input type="text" id="11" class="invent nonmetro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                December
                            </label>
                            <div class="col-md-4">
                                <input type="text" id="12" class="invent metro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                            <div class="col-md-4">
                                <input type="text" id="12" class="invent nonmetro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
                                January
                            </label>
                            <div class="col-md-4">
                                <input type="text" id="1" class="invent metro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                            <div class="col-md-4">
                                <input type="text" id="1" class="invent nonmetro form-control" placeholder="Enter the Value" maxlength="20" onkeypress="return $validator.IsNumeric(event, this.id)" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-4">
