@{
    ViewBag.Title = "LoanEntryList";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_HomeLayout.cshtml";
}
<h2>Loan Entry</h2>
<div class="row">
    <div class="form-horizontal">
        <div class="row">
            <div class="col-sm-12">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label col-md-4">
                            Category
                        </label>
                        <div class="col-md-6">
                            <select id="sltCategorylist" class="form-control" autofocus></select>
                        </div>
                    </div>
                    <div class="form-group" id="dvEmpCode">
                        <label class="control-label col-md-4">
                            Employee Code
                        </label>
                        <div class="col-md-6">
                            <select id="sltEmployeelist" class="form-control"></select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row" >
            <div class="col-sm-12" id="transHtml">

            </div>


        </div>
    </div>   
   
</div>


<script src="~/Js/Loan/LoanEntry.js" type="text/javascript"></script>
<script src="~/Js/Company/CompanyCom.js" type="text/javascript"></script>
@section Scripts
{
    <script src="~/Js/Loan/LoanEntry.js" type="text/javascript"></script>
    <script src="~/Js/Company/CompanyCom.js" type="text/javascript"></script>
}

