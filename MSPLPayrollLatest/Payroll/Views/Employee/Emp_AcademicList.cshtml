@{
    ViewBag.Title = "Emp_AcademicList";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_HomeLayout.cshtml";
}
<h4>
    Academic</h4>
<div class="row">
    <div class="col-md-12">
        <div class="form-group">
            <input type="button" id="btnAddAcademic" value="Add" class="btn custom-button pull-right" data-toggle="modal"
                   data-target="#AddAcademic">
        </div>
    </div>
    <div class="col-md-12">
        <div class="form-group table-responsive">
            <table id="tblAcademic" class="table table-responsive table-striped table-hover table-condensed userTablehand dataTable no-footer">
                <thead>
                    <tr>
                        <th class="nodisp">
                        </th>
                        <th class="nodisp">
                        </th>
                        <th>
                            Degree Name
                        </th>
                        <th>
                            Institution Name
                        </th>
                        <th>
                            Year Of Passing
                        </th>
                        <th>
                            Action
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>

                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div id="AddAcademic" class="modal fade" role="dialog">
        @Html.Partial("~/Views/Employee/Emp_Academic.cshtml")
    </div>
</div>
<script src="~/Js/Emp/academic.js" type="text/javascript"></script>
@section Scripts 
{
    <script src="~/Js/Emp/academic.js" type="text/javascript"></script>
}
