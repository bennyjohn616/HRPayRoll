
<div class="form-horizontal">
    <div class="row">
        <div class="col-md-10">
            <input type="button" id="btngeneratepdf" value="Generate PDF" onclick="$pfesiReport.GetReport()" class="btn custom-button pull-right">
        </div>
        <br />
        <br />
        <div class="col-md-10">
            <div class="col-sm-12">
                <input type="radio" name="reportname" value="pfForm3A" checked="checked" /> Form 3A
                &nbsp;&nbsp;&nbsp;
                <input type="radio" name="reportname" value="pfExtract" />PF Extract
                &nbsp;&nbsp;&nbsp;
                <input type="radio" name="reportname" value="pfChallan" />PF Challan
            </div>
        </div>
        <br />
        <br />
        <div class="col-md-10">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title" id="HeadPanel"> Form 3A </h3>
                </div>
                <div class="panel-body">

                    @*<div class="col-md-12">*@
                        <div class="col-md-12 nodisp" id="divSingleMonth">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="control-label col-md-4">
                                            Month
                                        </label>
                                        <div class="col-md-7">
                                            <select id="sltMonth" class="form-control">
                                                <option value="1">January</option>
                                                <option value="2">February</option>
                                                <option value="3">March</option>
                                                <option value="4">April</option>
                                                <option value="5">May</option>
                                                <option value="6">June</option>
                                                <option value="7">July</option>
                                                <option value="8">August</option>
                                                <option value="9">September</option>
                                                <option value="10">October</option>
                                                <option value="11">November</option>
                                                <option value="12">December</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="control-label col-md-4">
                                            Year
                                        </label>
                                        <div class="col-md-7">
                                            @*<input type="text" id="sltYear" onkeypress="return $validator.IsNumeric(event,this.id)" maxlength="4" />*@
                                            <select id="sltYear" class="form-control" autofocus></select>
                                        </div>
                                    </div>
                                </div>
                        </div>
                        <div class="col-md-12 " id="divPeriod">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="control-label col-md-4">
                                            Form
                                        </label>
                                        <div class="col-md-7">
                                            @*<input type="text" id="sltSYear" onkeypress="return $validator.IsNumeric(event,this.id)" maxlength="4" />*@
                                            <select id="sltSYear" class="form-control" autofocus></select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="control-label col-md-3">
                                            To
                                        </label>
                                        <div class="col-md-7">
                                            @*<input type="text" id="sltEYear" onkeypress="return $validator.IsNumeric(event,this.id)" maxlength="4" readonly />*@
                                            <select id="sltEYear" class="form-control" autofocus></select>
                                        </div>
                                    </div>
                                </div>
                        </div>
                    @*</div>*@

                    <div class="row" id="divempcode">
                        <div class="col-md-12">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label col-md-4">
                                        EmployeeCode
                                    </label>
                                    <div class="col-md-7">
                                        <input type="text" id="txtEmpcode" value="" class="form-control">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                            </div>
                        </div>
                    </div>

                    <div class="row nodisp" id="divpfChallan">
                        <div class="col-md-12">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label col-md-4">
                                        Cheque Date
                                    </label>
                                    <div class="col-md-7">
                                        <input type="text" id="dtpChqDate" class="form-control datepicker" placeholder="Select the Cheque Date" />

                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label col-md-4">
                                        Payment Date
                                    </label>
                                    <div class="col-md-7">
                                        <input type="text" id="dtpPayDate" class="form-control datepicker" placeholder="Select the Payment Date" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label col-md-4">
                                        Cheque Number
                                    </label>
                                    <div class="col-md-7">
                                        <input type="text" id="txtChqNo" class="form-control" placeholder="Enter the Cheque Number" />
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-5">
                <div class="col-md-8">
                    <div class="row" id="divCategory">
                            @Html.Partial("~/Views/DataWizard/Category.cshtml")
                    </div>
                </div>
    <div class="row" id="divFilter">

        <div class="form-horizontal">
            <div class="col-sm-12">
                <h4>Filter </h4>

                @Html.Partial("~/Views/DataWizard/Filters.cshtml")
            </div>
        </div>
    </div>
</div>
            <script>
                $companyCom.loadYear({ id: "sltYear" });
                $companyCom.loadYear({ id: "sltSYear" });
                $companyCom.loadYear({ id: "sltEYear" });
                $("#sltSYear").on("change", function () {
                    
                    var yearval = $("#sltSYear").val();
                    var EndYearVal = parseInt(yearval) + 1;
                    $("#sltEYear").val(EndYearVal);

                });
                $dwcategory.loadComponent();
                $(document).ready(function () {
                    
                    $("#dtpChqDate").datepicker();
                    $("#dtpPayDate").datepicker();
                    $('#rdEarnFilt').addClass('nodisp');
                    $('#rdDeducFilt').addClass('nodisp');
                    $('#txtEarnRd').addClass('nodisp');
                    $('#txtDeducRd').addClass('nodisp');
                });
            </script>

            <script src="~/Js/DataWizard/pfreport.js"></script>
            <script src="~/Js/Company/CompanyCom.js"></script>
            @section scripts
    {

                <script src="~/Js/DataWizard/pfreport.js"></script>
                <script src="~/Js/Company/CompanyCom.js"></script>
            }
